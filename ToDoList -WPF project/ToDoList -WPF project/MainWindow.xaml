<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ToDoList__WPF_project"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" x:Class="ToDoList__WPF_project.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
  
        <WrapPanel Orientation="Horizontal">
        <WrapPanel.OpacityMask>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="0"/>
                <GradientStop Color="#FFA63838" Offset="1"/>
            </LinearGradientBrush>
        </WrapPanel.OpacityMask>
        <Label Content="TO DO LIST"/>
        <Grid/>
        <Canvas Height="100" Width="100"/>
        <Image Height="29" Width="59" Source="{dx:DXImage SvgImages/XAF/State_Validation_Skipped.svg}"/>
        <Canvas Height="74" Width="74" OpacityMask="#FFAC3939"/>
        <Ellipse Fill="#FFF4F4F5" Height="226" Stroke="Black" Width="177"/>

    </WrapPanel>

</Window>
